{
  "version": 2,
  "builds": [
    {
      "src": "Back/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/login",
      "dest": "/api/login"
    },
    {
      "src": "/api/register",
      "dest": "/api/register"
    },
    {
      "src": "/api/categoria",
      "dest": "/api/categoria"
    },
    {
      "src": "/(.*)",
      "dest": "/Back/server.js"
    }
  ]
}